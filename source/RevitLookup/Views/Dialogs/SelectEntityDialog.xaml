<StackPanel
    x:Class="RevitLookup.Views.Dialogs.SelectEntityDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:dialogs="clr-namespace:RevitLookup.ViewModels.Dialogs.ExtensibleStorage"
    mc:Ignorable="d"
    MinWidth="416"
    d:DataContext="{d:DesignInstance dialogs:SelectEntityViewModel}">
    <ComboBox
        IsReadOnly="True"
        SelectedItem="{Binding SelectedEntity}"
        ItemsSource="{Binding ExistedEntities}">
        <ComboBox.ItemTemplate>
            <DataTemplate>
                <TextBlock Text="{Binding EntityName}"/>
            </DataTemplate>
        </ComboBox.ItemTemplate>
    </ComboBox>
</StackPanel>