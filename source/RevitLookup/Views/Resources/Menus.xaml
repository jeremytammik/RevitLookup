<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:rl="http://revitlookup.com/xaml">
    <MenuItem
        x:Key="Label"
        x:Shared="False"
        FontWeight="DemiBold"
        Focusable="False"
        IsHitTestVisible="False" />
    <rl:MenuItem
        x:Key="CheckableMenuItem"
        x:Shared="False"
        IsCheckable="True" />
    <rl:MenuItem
        x:Key="CopyMenuItem"
        x:Shared="False"
        Header="Copy"
        Command="ApplicationCommands.Copy"
        Icon="{rl:SymbolIcon Copy16}" />
    <rl:MenuItem
        x:Key="HelpMenuItem"
        x:Shared="False"
        Header="Help"
        Icon="{rl:SymbolIcon QuestionCircle16}" />
    <rl:MenuItem
        x:Key="RefreshMenuItem"
        x:Shared="False"
        Header="Refresh"
        Icon="{rl:SymbolIcon ArrowCounterclockwise12}" />
    <rl:MenuItem
        x:Key="ShowTimeMenuItem"
        x:Shared="False"
        Header="Show/hide time"
        Icon="{rl:SymbolIcon Timer12}" />
</ResourceDictionary>