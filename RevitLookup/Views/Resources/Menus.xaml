<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:rl="http://revitlookup.com/xaml"
    x:Class="RevitLookup.Views.Resources.Menus">
    <MenuItem
        x:Key="Label"
        x:Shared="False"
        FontWeight="DemiBold"
        Focusable="False"
        IsHitTestVisible="False">
    </MenuItem>
    <rl:MenuItem
        x:Key="CheckableMenuItem"
        x:Shared="False"
        IsCheckable="True">
    </rl:MenuItem>
    <rl:MenuItem
        x:Key="CopyMenuItem"
        x:Shared="False"
        Header="Copy"
        SymbolIcon="Copy16"
        Command="ApplicationCommands.Copy">
    </rl:MenuItem>
    <rl:MenuItem
        x:Key="HelpMenuItem"
        x:Shared="False"
        Header="Help"
        SymbolIcon="QuestionCircle16">
    </rl:MenuItem>
    <rl:MenuItem
        x:Key="RefreshMenuItem"
        x:Shared="False"
        Header="Refresh"
        SymbolIcon="ArrowCounterclockwise12">
    </rl:MenuItem>
    <rl:MenuItem
        x:Key="ShowTimeMenuItem"
        x:Shared="False"
        Header="Show/hide time"
        SymbolIcon="Timer12">
    </rl:MenuItem>
</ResourceDictionary>